{
  "compilerOptions": {
    "target": "ES5",
    "lib": [
      "ES2021",
    ],
    "types": [],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true
  },
  "exclude": [
    "node_modules",
    "dist",
  ],
  "include": [
    "src",
  ],
}