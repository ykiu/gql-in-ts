{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "lib": [
      "ES5",
    ],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "declaration": true,
    "downlevelIteration": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src"
  ],
}